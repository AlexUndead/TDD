[Unit]
Description=Gunicorn server for SITENAME

[Service]
Restart=on-failure
User=ubuntu
WorkingDirectory=/home/ubuntu/sites/SITENAME/source
ExecStart=/home/ubuntu/sites/TDD-staging/virtualenv/bin/gunicorn --bind unix:/tmp/TDD-staging.socket --capture-output --access-logfile ../access.log --error-logfile ../error.log superlists.wsgi:application

[Install]
WantedBy=multi-user.target
